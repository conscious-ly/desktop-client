    "@vue/test-utils": "1.0.0-beta.29",
    "babel-eslint": "^10.0.3",
    "electron-icon-builder": "^1.0.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "lint-staged": "^9.4.2",
    "sass": "^1.19.0",
    "sass-loader": "^8.0.0",
    "vue-template-compiler": "^2.6.10"
