    "@fortawesome/vue-fontawesome": "^0.1.7",
    "@innologica/vue-dropdown-menu": "^0.1.3",
    "chokidar": "^3.2.2",
    "core-js": "^3.3.2",
    "electron": "^6.0.0",
    "v-tooltip": "^2.0.2",
    "vue": "^2.6.10",
    "vue-circle-slider": "^1.0.2",
    "vue-cli-plugin-electron-builder": "^1.4.0",
    "vue-router": "^3.1.3",
    "vue-select": "^3.2.0",
    "vuex": "^3.0.1",
